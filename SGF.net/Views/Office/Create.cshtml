@model SGF.net.Models.offices

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm(new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <div class="form-group">
        @Html.LabelFor(model => model.officeId, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(model => model.officeId, new { @class = "form-control" })
            <br />
            @Html.ValidationMessageFor(model => model.officeId)
        </div>
    </div>
    
     <div class="form-group">
        @Html.LabelFor(model => model.officeName, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(model => model.officeName, new { @class = "form-control" })
            <br />
            @Html.ValidationMessageFor(model => model.officeName)
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="Create" />            
            @Html.ActionLink("Back to list", "Index", null, null, new { @class = "btn btn-default" })
        </div>
        
    </div>
       
   
}

<div>
